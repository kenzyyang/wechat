.list-main {
  width: 99%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.list {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  width: 94%;
  height: 600rpx;
  margin-top: 25rpx;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.3);
  background-color: #fff;
  border-radius: 15rpx;
}

.list .list-item {
  width: 100%;
  height: 100rpx;
  padding: 0 40px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  color: #8799a3;
}

.list .list-item {
  border-bottom: 1px solid #f0f0f0;
}
